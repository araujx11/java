package fundamentos;
import java.util.Scanner;
public class crime {
	
	
	

	public static void main(String[] args) {
		double contador = 0;
		System.out.println("digite 1 para sim e 2 para nao.");
		System.out.println("telefonou para a vitmima? ");
		Scanner tell=new Scanner(System.in);
		double resp1= tell.nextDouble();
		if (resp1 == 1)
		contador = contador + 1;
		else if (resp1==2)
			System.out.println("nao");
		 
		System.out.println("esteve no local do crime?");
		Scanner local=new Scanner(System.in);
		double resp2= local.nextDouble();
		if (resp2==1)
			 contador= contador+1;
		else if (resp2==2)
			System.out.println("nao");
	
		System.out.println("mora perto da vitima?");
		Scanner perto=new Scanner(System.in);
		double resp3= perto.nextDouble();
		if (resp3==1)
			 contador= contador+1;
		else if (resp3==2)
			System.out.println("nao");
		
		System.out.println("devia para a vitima?");
		Scanner devia=new Scanner(System.in);
		double resp4= devia.nextDouble();
		
		if (resp4==1)
			 contador= contador+1;
		else if (resp4==2)
			System.out.println("nao");
		System.out.println("ja tralhou com a vitima?");
		Scanner trabalho=new Scanner(System.in);
		double resp5= trabalho.nextDouble();
		if (contador==5)
		System.out.println("assasino");
		else if (contador>=3)
		System.out.println("cumplice");
		else 
			System.out.println("suspeito");
		
	}

}
